{
  "name": "capacitor-site",
  "private": true,
  "version": "0.0.1",
  "description": "Capacitor Site",
  "scripts": {
    "api-typedoc": "typedoc --exclude **/__test__/*.ts --json dist/docs.json --mode modules --includeDeclarations node_modules/@capacitor/core/dist/esm/core-plugin-definitions.d.ts",
    "postapi-typedoc": "ts-node scripts/filter-docs.ts",
    "ci:env": "touch .env && echo PRISMIC_ENDPOINT=$PRISMIC_ENDPOINT >> .env",
    "ci": "npm run ci:env && npm run build",
    "build": "npm run api-typedoc && stencil build --prerender --ci",
    "prerender": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve --ssr",
    "generate": "stencil generate",
    "serve": "stencil build --dev --watch",
    "v2": "ts-node --project scripts/tsconfig.json scripts/plugin-api-v2.ts"
  },
  "devDependencies": {
    "@capacitor/core": "^2.4.2",
    "@capacitor/docgen": "^0.0.4",
    "@ionic-internal/ionic-ds": "2.1.2",
    "@ionic/prettier-config": "^1.0.1",
    "@stencil/core": "^2.1.0-2",
    "@stencil/helmet": "^0.3.2",
    "@stencil/sass": "^1.3.2",
    "@stencil/ssg": "^0.0.15",
    "@stencil/store": "^1.3.0",
    "@types/node-fetch": "^2.5.7",
    "@types/prismjs": "^1.16.1",
    "copy-text-to-clipboard": "^1.0.4",
    "date-fns": "^2.14.0",
    "dotenv": "^8.0.0",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "ionicons": "^5.0.1",
    "natives": "^1.1.6",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "prismic-dom": "^2.2.4",
    "prismic-javascript": "^2.7.1",
    "prismjs": "^1.21.0",
    "read-pkg-up": "^7.0.1",
    "rollup-plugin-dotenv": "^0.3.0",
    "stencil-router-v2": "^0.5.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.17.7",
    "typescript": "^4.0.3"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/capacitor"
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "homepage": "https://github.com/ionic-team/capacitor",
  "prettier": "@ionic/prettier-config"
}
